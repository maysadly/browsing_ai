# Language model provider: openai | anthropic
LLM_PROVIDER=openai

# API keys (set at runtime)
OPENAI_API_KEY=your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key

# Sandbox configuration
ALLOWLIST_HOSTS=duckduckgo.com,example.com,github.com,nytimes.com
MAX_STEPS=40
STEP_TIMEOUT_S=30
RUN_TIMEOUT_S=600
ALLOW_JS_EVAL=false
ALLOW_FILE_UPLOAD=false
HEADLESS_DEFAULT=true

# Logging & tracing
LOG_LEVEL=INFO
LOG_DIR=logs
RUNS_DIR=runs

# FastAPI server
HOST=0.0.0.0
PORT=8000
